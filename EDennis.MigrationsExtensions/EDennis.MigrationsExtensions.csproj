<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <Version>1.3.0</Version>
    <Description>This package provides the ability to generate SQL Server temporal/history tables during Entity Framework Core migrations.</Description>
    <PackageReleaseNotes>Upgraded to .NET Core 2.1</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageIconUrl>https://avatars0.githubusercontent.com/u/2938333?s=400&amp;v=4</PackageIconUrl>
    <FileVersion>1.3.0.0</FileVersion>
    <AssemblyVersion>1.3.0.0</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="CreateMaintenanceSchema.sql" />
    <None Remove="CreateMaintenanceSchema_Drop.sql" />
    <None Remove="CreateTestJsonTable.sql" />
    <None Remove="CreateTestJsonTable_Drop.sql" />
    <None Remove="ResetSequences.sql" />
    <None Remove="ResetSequences_Drop.sql" />
    <None Remove="SaveTestJson.sql" />
    <None Remove="SaveTestJson_Drop.sql" />
    <None Remove="Temporal_AddHistoryTables.sql" />
    <None Remove="Temporal_AddHistoryTables_Drop.sql" />
    <None Remove="Temporal_DisableSystemTime.sql" />
    <None Remove="Temporal_DisableSystemTime_Drop.sql" />
    <None Remove="Temporal_EnableSystemTime.sql" />
    <None Remove="Temporal_EnableSystemTime_Drop.sql" />
    <None Remove="Temporal_GetMetadataFromExtProp.sql" />
    <None Remove="Temporal_GetMetadataFromExtProp_Drop.sql" />
    <None Remove="Temporal_GetMetadataFromInfoSchema.sql" />
    <None Remove="Temporal_GetMetadataFromInfoSchema_Drop.sql" />
    <None Remove="Temporal_UpdateExtendedProperties.sql" />
    <None Remove="Temporal_UpdateExtendedProperties_Drop.sql" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="CreateTestJsonTable_Drop.sql" />
    <EmbeddedResource Include="CreateTestJsonTable.sql" />
    <EmbeddedResource Include="SaveTestJson.sql" />
    <EmbeddedResource Include="Temporal_AddHistoryTables.sql" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="ResetSequences.sql">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="ResetSequences_Drop.sql" />
    <EmbeddedResource Include="SaveTestJson_Drop.sql" />
    <EmbeddedResource Include="Temporal_AddHistoryTables_Drop.sql" />
    <EmbeddedResource Include="CreateMaintenanceSchema.sql" />
    <EmbeddedResource Include="CreateMaintenanceSchema_Drop.sql" />
    <EmbeddedResource Include="Temporal_DisableSystemTime.sql" />
    <EmbeddedResource Include="Temporal_DisableSystemTime_Drop.sql" />
    <EmbeddedResource Include="Temporal_EnableSystemTime.sql" />
    <EmbeddedResource Include="Temporal_EnableSystemTime_Drop.sql" />
    <EmbeddedResource Include="Temporal_GetMetadataFromExtProp.sql" />
    <EmbeddedResource Include="Temporal_GetMetadataFromExtProp_Drop.sql" />
    <EmbeddedResource Include="Temporal_GetMetadataFromInfoSchema.sql" />
    <EmbeddedResource Include="Temporal_GetMetadataFromInfoSchema_Drop.sql" />
    <EmbeddedResource Include="Temporal_UpdateExtendedProperties.sql" />
    <EmbeddedResource Include="Temporal_UpdateExtendedProperties_Drop.sql" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="2.1.0" />
  </ItemGroup>

</Project>
